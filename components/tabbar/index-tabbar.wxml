<!--tabbar-->
<view style="height:20vh">
	<van-tabbar active="{{ active }}" active-color="#f59739" inactive-color="#CDCDC1" bind:change="onChange">
	<van-tabbar-item
				wx:for="{{panels}}"
				wx:for-item="item"
				name="{{item.name}}"
				icon="{{item.icon}}"
				dot="{{item.dot}}">
				{{item.label}}
			</van-tabbar-item>
	</van-tabbar>
</view>
