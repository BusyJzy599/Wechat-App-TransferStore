/* components/tabbar/index-tabbar.wxss */
.tabbar-index--van-tabbar {
    display: flex;
    width: 100%;
    padding-top: 18rpx;
    height: var(--tabbar-height, 80px);
    background-color: #fff;
    background-color: var(--tabbar-background-color, #fff);
}

.tabbar-index--van-tabbar--safe {
    height: var(--tabbar-height, 80px);
    padding-top: 20rpx;
}